@page
@model E_Migrant.App.FrontEnd.EliminarAmigoModel
@{
}
<div class="center">
<h3>Eliminar Amigo - @Model.amigo.Id</h3>
<form method="post" class="mt-3">
  
    <input hidden asp-for="amigo.Id">
    <h1>Va a eliminar el amigo selecionado esta seguro?</h1>
    
    <div>
        <button type="submit" class="btn btn-primary">Eliminar</button>
        <a asp-page="./ListarVehiculo" class="btn btn-primary">Cancelar</a>
    </div>
</form>
</div>
